{
	"expo": {
		"name": "music-app",
		"slug": "music-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "dark",
		"scheme": "music-app",
		"newArchEnabled": false,
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#000",
			"infoPlist": {
				"UIBackgoundModes": ["audio"]
			}
		},
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#000",
				"infoPlist": {
					"UIBackgoundModes": ["audio"]
				}
			},
			"package": "com.anonymous.musicapp"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true,
			"tsconfigPaths": true
		}
	}
}
